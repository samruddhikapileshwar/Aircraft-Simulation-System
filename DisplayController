	public void submitButtonActionPerformed(java.awt.event.ActionEvent evt) {
		try {

			speed = Integer.parseInt(speedTextField.getText());
			altitude = Integer.parseInt(altitudeTextField.getText());
			timeToLand = Integer.parseInt(timeToLandTextField.getText());
			gearPosition = (String) gearPositionComboBox.getSelectedItem();

			ComputeSimulationState csc = new ComputeSimulationState();
			csc.computeSimulation(speed, altitude, gearPosition, timeToLand);
			dispose();

		}

		catch (NumberFormatException nfe) {

			if (speedTextField.getText().equals("")) {
				messageTextArea.setText("Enter speed");
			}

			if (altitudeTextField.getText().equals("")) {
				messageTextArea.setText("Enter Altitude\nEnter Speed");
			}
			if (timeToLandTextField.getText().equals("")) {
				messageTextArea
						.setText("Enter Time to Land\nEnter Altitude\nEnter Speed");
			}

		}

	}

	Clip clip = null;

	public void playAlarm(boolean playAlarm) {

		String soundName = "alarm.wav";
		AudioInputStream audioInputStream = null;
		try {
			audioInputStream = AudioSystem.getAudioInputStream(new File(
					soundName).getAbsoluteFile());
		} catch (UnsupportedAudioFileException | IOException e) {
			e.printStackTrace();
		}

		try {
			clip = AudioSystem.getClip();
		} catch (LineUnavailableException e) {

			e.printStackTrace();
		}
		try {

			if (playAlarm == true) {
				clip.open(audioInputStream);
				clip.loop(10);

			}

		} catch (LineUnavailableException | IOException e) {

			e.printStackTrace();
		}

	}

	public void finalDisplay(boolean gear_override_warning_on,
			boolean airbrake_warning_on, boolean gear_not_down_alarm_on,
			boolean gear_air_speed_alarm_on) {
		setVisible(true);
		String warningText1 = "", warningText2 = "";
		String alarmText1 = "", alarmText2 = "";
		if (gear_override_warning_on == true) {
			warningText1 = "Gear Override Warning\n ";
		}
		if (airbrake_warning_on == true) {
			warningText2 = "Air Break Warning";
		}

		warningTextArea.setText(warningText1 + warningText2);

		if (gear_not_down_alarm_on == true) {
			alarmText1 = "Gear Not Down Alarm";

		}
		if (gear_air_speed_alarm_on == true) {
			alarmText2 = "Gear Air Speed Alarm";

		}

		alarmTextArea.setText(alarmText1 + alarmText2);

		if (gear_not_down_alarm_on == true || gear_air_speed_alarm_on == true) {
			playAlarm(true);
		}
	}

	public void messageDisplay(int speed, int timeToLand, String gearPosition,
			int altitude, boolean gear_override_warning_on,
			boolean airbrake_warning_on, boolean gear_not_down_alarm_on,
			boolean gear_air_speed_alarm_on) {
		String message = "Gear Position: " + gearPosition + "\nTime To Land: "
				+ timeToLand + "\nAltitude: " + altitude2 + "\nSpeed: "
				+ speed2 + "\nAirBrake warning: " + airbrake_warning_on
				+ "\nGear Override Warning on: " + gear_override_warning_on
				+ "\nGear Not Down Alarm on: " + gear_not_down_alarm_on
				+ "\nGear Air Speed Alarm on: " + gear_air_speed_alarm_on;

		altitude2 = altitude;
		speed2 = speed;

		altitudeTextField.setText(Integer.toString(altitude2));
		timeToLandTextField.setText(Integer.toString(timeToLand));
		gearPositionComboBox.setSelectedItem(gearPosition);
		speedTextField.setText(Integer.toString(speed2));

		messageTextArea.setText(message);
	}

	private void clearButtonActionPerformed(java.awt.event.ActionEvent evt) {
		altitudeTextField.setText("");
		speedTextField.setText("");
		timeToLandTextField.setText("");
		alarmTextArea.setText("");
		warningTextArea.setText("");
		messageTextArea.setText("");

	}

	private void alarmButtonActionPerformed(ActionEvent evt) {
		clip.stop();

	}

	private void altitudeDecreaseButtonActionPerformed(
			java.awt.event.ActionEvent evt) {

		altitude2 = altitude2 - 20;
		altitudeTextField.setText("" + altitude2);
	}

	private void altitudeIncreaseButtonActionPerformed(
			java.awt.event.ActionEvent evt) {
		altitude2 = altitude2 + 20;
		altitudeTextField.setText("" + altitude2);
	}

	public void speedDecreaseButtonActionPerformed(
			java.awt.event.ActionEvent evt) {
		speed2 = speed2 - 10;
		speedTextField.setText("" + speed2);
	}

	private void exitButtonActionPerformed(ActionEvent evt) {

		dispose();
	}

	private void speedIncreaseButtonActionPerformed(
			java.awt.event.ActionEvent evt) {
		speed2 = speed2 + 10;
		speedTextField.setText("" + speed2);
	}

